<!-- Keep pu income for loss of capital -->
    <!-- retainCapitalNumber means that we do not lose our money if we still control at least this many of our capitals (defaults to 1).
		If a player has no playerAttachment, then it defaults to X, where X = total number of capitals
		(ie: you lose your money if a single capital is captumori) -->
    <!-- retainCapitalProduceNumber is similar to above, but only affect gaining income and producing units (probably should be less 
		than or equal to retainCapitalNumber)
		If a player has no playerAttachment, then it defaults to 1 (ie: you keep collecting and producing units so long as you control at least 1 capital) -->
    <attachment name="playerAttachment" attachTo="Date" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/>
		<option name="retainCapitalProduceNumber" value="0"/>
    </attachment>
    <attachment name="playerAttachment" attachTo="Shimazu" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/>
		<option name="retainCapitalProduceNumber" value="0"/>
    </attachment>
    <attachment name="playerAttachment" attachTo="Mori" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/> 			<!-- Do not lose PUs as long as we control this many of our capitals -->
		<option name="retainCapitalProduceNumber" value="0"/> 	<!-- Keep collecting/producing units so long as you control at least 0 capitals -->
    </attachment>
    <attachment name="playerAttachment" attachTo="Miyoshi" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/>
		<option name="retainCapitalProduceNumber" value="0"/>
    </attachment>
    <attachment name="playerAttachment" attachTo="Hojo" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/>
		<option name="retainCapitalProduceNumber" value="0"/>
    </attachment>
    <attachment name="playerAttachment" attachTo="Oda" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/>
		<option name="retainCapitalProduceNumber" value="0"/>
    </attachment>
    <attachment name="playerAttachment" attachTo="Rebels" javaClass="games.strategy.triplea.attachments.PlayerAttachment" type="player">
		<option name="retainCapitalNumber" value="0"/>
		<option name="retainCapitalProduceNumber" value="0"/>
